type Main class {
    main() void {
        var test[10] int;
        
        for i := 0; i < 10; i = i+1 {
            test[9-i] = i*2;
        }

        print("Unsorted array:");
        for i := 0; i < 10; i = i+1 {
            print(test[i]);
        }
        
        sort(test, "asc");
        print("Sorted array:");
        for i := 0; i < 10; i = i+1 {
            print(test[i]);
        }

        print("Find Index: " + toString(find(test, 2+2)));
    }
}
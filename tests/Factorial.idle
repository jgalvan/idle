type Factorial class {
    Recurssion(x int) int {
        if x == 0 {
            return 1;
        }
        else if x == 1 {
            return 1;
        }
        return x * Recurssion(x-1);
    }

    Loops(x int) int {
        if (x == 0) {
            return 1;
        }
        factorial := 1;
        while x != 1 {
            factorial = factorial * x;
            x = x - 1;
        }
        return factorial;
    }
}

type Main class {
    main() void
    {
        fac := new Factorial();
        print("Factorial");
        print("Recurssion: " + toString(fac.Recurssion(6)));
        print("Cicles: " + toString(fac.Loops(6)));
    }
}
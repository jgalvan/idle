type Animal class {
    name string;
    age int;

    GetName() string {
        return $name;
    }
}


type Dog <- Animal class {
    breed string;

    Bark(woof string) bool {
        if $age > 1 {
            IO.print(GetName());
            return true;
        }
        return false;
    }
}


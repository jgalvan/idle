type Animal class {
    - name string;
    - age int;

    - GetName() string {
        return $name;
    }

    + SetName(name string) void {
        $name = name;
    }
}

type Dog <- Animal class {
    - breed string;

    - bark() bool {
        print(GetName());
        if $age > 1 {
            print("WOOF");
            return true;
        }
        print("woof");
        return false;
    }

    + bark2() bool {
        return bark();
    }

    + setAge(age int) void {
        $age = age;
    }

    + Dog(name string, breed string)
    {
        var mydog Dog;
        print("Corri");
        mydog.SetName(name);
        return mydog;
    }
}


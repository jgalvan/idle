type Animal class {
    name string;
    age int;

    getName() string {
        return $name;
    }

    setName(name string) void {
        $name = name;
    }

    doSomething() void {
        IO.print("ANIMAL");
    }
}

type Mammal <- Animal class {
    doSomething() void {
        IO.print("MAMMAL");
    }
}

type Dog <- Mammal class {
    other string;

    Dog(name string, age int)
    {
        $name = name;
        $age = age;
    }

    bark() void {
        IO.print("BARK!");
    }

    setOther(other string) void {
        $other = other;
    }

    getOther() string {
        return $other;
    }

    doSomething() void {
        IO.print("DOG");
        super().doSomething();
    }
}

type Main class {
    main() void
    {
        var test Dog;

        test = new Dog("test", 18);
        IO.print(test.getName());
        test.setOther("other");
        IO.print(test.getOther());

        test = new Dog("test2", 18);
        IO.print(test.getName());
        test.setOther("other2");
        IO.print(test.getOther());

        test.bark();

        var test3 Animal;
        test3 = new Animal();
        test3.setName("test3");
        IO.print(test3.getName());


        test.doSomething();
        test3.doSomething();
    }
}
